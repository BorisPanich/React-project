v className={s.dialogsItem}>\r\n                {dialogsElement}\r\n            </div>\r\n            <div className={s.messages}>\r\n                <div>{messagesElement}</div>\r\n                <div className={s.addMessage}>\r\n                </div>\r\n                <AddMessageFormRedux onSubmit={addNewMessage}/>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\ntype FormDataType = {\r\n    newMessageText: string\r\n}\r\n\r\nconst maxLength20 = maxLength(20);\r\nconst minLength2 = minLength(2, 20);\r\n\r\nconst AddMessageForm: React.FC<InjectedFormProps<FormDataType>> = (props) => {\r\n    return (\r\n        <form onSubmit={props.handleSubmit}>\r\n            <div className={s.messageInput}>\r\n                <Field component={FormElementTextarea}\r\n                       validate={[required, maxLength20, minLength2]}\r\n                       placeholder=\"send Message here\"\r\n                       name=\"newMessageText\"/>\r\n                <div>\r\n                    <button type={'submit'}>add message</button>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    )\r\n}\r\nconst AddMessageFormRedux = reduxForm<FormDataType>({form: \"dialogAddMessageForm\"})(AddMessageForm)\r\n\r\nexport default Dialogs;",["197","198"],"D:\\boris\\programming\\it-incubator\\react-js\\01-start\\react-01\\src\\components\\Navbar\\Navbar.tsx",[],"D:\\boris\\programming\\it-incubator\\react-js\\01-start\\react-01\\src\\components\\News\\News.tsx",[],"D:\\boris\\programming\\it-incubator\\react-js\\01-start\\react-01\\src\\redux\\profileReducer.tsx",["199"],"D:\\boris\\programming\\it-incubator\\react-js\\01-start\\react-01\\src\\components\\Profile\\MyPost\\MyPost.tsx",[],"D:\\boris\\programming\\it-incubator\\react-js\\01-start\\react-01\\src\\components\\Profile\\MyPost\\MyPostContainer.tsx",["200"],"import React from 'react';\nimport MyPost from \"./MyPost\";\nimport { RootReduxState} from \"../../../redux/redux-store\";\nimport {conne